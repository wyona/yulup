<?xml version="1.0" encoding="UTF-8"?>

<dtd xmlns="http://yulup.wyona.org/1.0">

  <entity id="editorConfirmCloseDialog.title">
    <name xml:lang="en">Close Yulup</name>
    <name xml:lang="de">Yulup beenden</name>
    <name xml:lang="es">Cerrar Yulup</name>
  </entity>

  <entity id="editorConfirmCloseDescription.label">
    <name xml:lang="en">The document has been changed. Really exit the editor?</name>
    <name xml:lang="de">Das Dokument wurde verändert. Soll der Editor wirklich beendet werden?</name>
    <name xml:lang="es">El documento ha sido modificado. ¿Desea realmente salir del editor?</name>
  </entity>

  <entity id="editorConfirmCloseCancelButton.label">
    <name xml:lang="en">Exit</name>
    <name xml:lang="de">Beenden</name>
    <name xml:lang="es">Salir</name>
  </entity>

  <entity id="editorConfirmCloseAcceptButton.label">
    <name xml:lang="en">Save and Exit</name>
    <name xml:lang="de">Speichern und beenden</name>
    <name xml:lang="es">Guardar y salir</name>
  </entity>

  <entity id="editorConfirmCloseCancelButton.tooltip">
    <name xml:lang="en">Exit without saving</name>
    <name xml:lang="de">Beenden ohne zu Speichern</name>
    <name xml:lang="es">Salir sin guardar</name>
  </entity>

  <entity id="editorConfirmCloseAcceptButton.tooltip">
    <name xml:lang="en">Execute the selected save action</name>
    <name xml:lang="de">Die ausgewählte Aktion ausführen</name>
    <name xml:lang="es">Ejecutar la acción de guardar seleccionada</name>
  </entity>

  <entity id="confirmCloseMenulist.accesskey">
    <name xml:lang="en">S</name>
  </entity>

</dtd>
